{
    "name": "compose-services",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev:connector": "tsx watch connector/src/server.ts",
        "dev:sandbox": "tsx watch sandbox/src/server.ts",
        "dev:exporter": "tsx watch exporter/src/server.ts",
        "dev": "concurrently \"npm:dev:*\"",
        "build:connector": "tsc -p connector/tsconfig.json",
        "build:sandbox": "tsc -p sandbox/tsconfig.json",
        "build:exporter": "tsc -p exporter/tsconfig.json",
        "build": "npm run build:connector && npm run build:sandbox && npm run build:exporter",
        "start:connector": "node connector/dist/server.js",
        "start:sandbox": "node sandbox/dist/server.js",
        "start:exporter": "node exporter/dist/server.js"
    },
    "dependencies": {
        "@elizaos/api-client": "^1.6.5",
        "@elizaos/plugin-node": "^0.25.6-alpha.1",
        "@elizaos/server": "^1.6.5",
        "@goat-sdk/adapter-model-context-protocol": "^0.2.11",
        "@goat-sdk/adapter-vercel-ai": "^0.2.10",
        "@goat-sdk/core": "^0.5.0",
        "@goat-sdk/plugin-coingecko": "^0.2.10",
        "@goat-sdk/plugin-erc20": "^0.2.14",
        "@goat-sdk/plugin-uniswap": "^0.2.16",
        "@goat-sdk/wallet-viem": "^0.3.0",
        "@modelcontextprotocol/sdk": "^1.24.3",
        "archiver": "^7.0.1",
        "cors": "^2.8.5",
        "dotenv": "^17.2.3",
        "eventsource": "^4.1.0",
        "express": "^5.2.1",
        "mem0ai": "^1.0.39",
        "thirdweb": "^5.116.1",
        "viem": "^2.42.0",
        "zod": "^4.1.13"
    },
    "devDependencies": {
        "@types/archiver": "^7.0.0",
        "@types/connect": "^3.4.38",
        "@types/cors": "^2.8.19",
        "@types/eventsource": "^3.0.0",
        "@types/express": "^5.0.6",
        "@types/node": "^25.0.2",
        "concurrently": "^9.2.1",
        "tsx": "^4.21.0",
        "typescript": "^5.9.3"
    }
}
